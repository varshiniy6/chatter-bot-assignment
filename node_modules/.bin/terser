#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/workspace/reactjs/sessions/authentication-and-authorisation/react-coding-challenges/chatter/node_modules/.pnpm/terser@5.9.0/node_modules/terser/bin/terser/node_modules:/home/workspace/reactjs/sessions/authentication-and-authorisation/react-coding-challenges/chatter/node_modules/.pnpm/terser@5.9.0/node_modules/terser/bin/node_modules:/home/workspace/reactjs/sessions/authentication-and-authorisation/react-coding-challenges/chatter/node_modules/.pnpm/terser@5.9.0/node_modules/terser/node_modules:/home/workspace/reactjs/sessions/authentication-and-authorisation/react-coding-challenges/chatter/node_modules/.pnpm/terser@5.9.0/node_modules:/home/workspace/reactjs/sessions/authentication-and-authorisation/react-coding-challenges/chatter/node_modules/.pnpm/node_modules:/home/workspace/reactjs/sessions/authentication-and-authorisation/react-coding-challenges/chatter/node_modules:/home/workspace/reactjs/sessions/authentication-and-authorisation/react-coding-challenges/node_modules:/home/workspace/reactjs/sessions/authentication-and-authorisation/node_modules:/home/workspace/reactjs/sessions/node_modules:/home/workspace/reactjs/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/reactjs/sessions/authentication-and-authorisation/react-coding-challenges/chatter/node_modules/terser/bin/terser/node_modules:/home/workspace/reactjs/sessions/authentication-and-authorisation/react-coding-challenges/chatter/node_modules/terser/bin/node_modules:/home/workspace/reactjs/sessions/authentication-and-authorisation/react-coding-challenges/chatter/node_modules/terser/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../terser/bin/terser" "$@"
else 
  exec node  "$basedir/../terser/bin/terser" "$@"
fi
