#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/workspace/reactjs/sessions/authentication-and-authorisation/react-coding-challenges/chatter/node_modules/.pnpm/in-publish@2.0.1/node_modules/in-publish/in-install.js/node_modules:/home/workspace/reactjs/sessions/authentication-and-authorisation/react-coding-challenges/chatter/node_modules/.pnpm/in-publish@2.0.1/node_modules/in-publish/node_modules:/home/workspace/reactjs/sessions/authentication-and-authorisation/react-coding-challenges/chatter/node_modules/.pnpm/in-publish@2.0.1/node_modules:/home/workspace/reactjs/sessions/authentication-and-authorisation/react-coding-challenges/chatter/node_modules/.pnpm/node_modules:/home/workspace/reactjs/sessions/authentication-and-authorisation/react-coding-challenges/chatter/node_modules:/home/workspace/reactjs/sessions/authentication-and-authorisation/react-coding-challenges/node_modules:/home/workspace/reactjs/sessions/authentication-and-authorisation/node_modules:/home/workspace/reactjs/sessions/node_modules:/home/workspace/reactjs/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/reactjs/sessions/authentication-and-authorisation/react-coding-challenges/chatter/node_modules/.pnpm/node-sass@4.14.1/node_modules/node-sass/node_modules:/home/workspace/reactjs/sessions/authentication-and-authorisation/react-coding-challenges/chatter/node_modules/.pnpm/node-sass@4.14.1/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../in-publish@2.0.1/node_modules/in-publish/in-install.js" "$@"
else 
  exec node  "$basedir/../../../../../in-publish@2.0.1/node_modules/in-publish/in-install.js" "$@"
fi
