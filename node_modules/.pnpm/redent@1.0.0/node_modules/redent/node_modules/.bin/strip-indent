#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/workspace/reactjs/sessions/authentication-and-authorisation/react-coding-challenges/chatter/node_modules/.pnpm/strip-indent@1.0.1/node_modules/strip-indent/cli.js/node_modules:/home/workspace/reactjs/sessions/authentication-and-authorisation/react-coding-challenges/chatter/node_modules/.pnpm/strip-indent@1.0.1/node_modules/strip-indent/node_modules:/home/workspace/reactjs/sessions/authentication-and-authorisation/react-coding-challenges/chatter/node_modules/.pnpm/strip-indent@1.0.1/node_modules:/home/workspace/reactjs/sessions/authentication-and-authorisation/react-coding-challenges/chatter/node_modules/.pnpm/node_modules:/home/workspace/reactjs/sessions/authentication-and-authorisation/react-coding-challenges/chatter/node_modules:/home/workspace/reactjs/sessions/authentication-and-authorisation/react-coding-challenges/node_modules:/home/workspace/reactjs/sessions/authentication-and-authorisation/node_modules:/home/workspace/reactjs/sessions/node_modules:/home/workspace/reactjs/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/reactjs/sessions/authentication-and-authorisation/react-coding-challenges/chatter/node_modules/.pnpm/redent@1.0.0/node_modules/redent/node_modules:/home/workspace/reactjs/sessions/authentication-and-authorisation/react-coding-challenges/chatter/node_modules/.pnpm/redent@1.0.0/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../strip-indent@1.0.1/node_modules/strip-indent/cli.js" "$@"
else 
  exec node  "$basedir/../../../../../strip-indent@1.0.1/node_modules/strip-indent/cli.js" "$@"
fi
